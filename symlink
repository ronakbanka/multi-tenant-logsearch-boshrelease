#!/bin/bash

cd jobs

ln -fs ../vendor/logsearch-boshrelease/jobs/api
ln -fs ../vendor/logsearch-boshrelease/jobs/archiver
ln -fs ../vendor/logsearch-boshrelease/jobs/elasticsearch
ln -fs ../vendor/logsearch-boshrelease/jobs/ingestor_archiver
ln -fs ../vendor/logsearch-boshrelease/jobs/ingestor_lumberjack
ln -fs ../vendor/logsearch-boshrelease/jobs/ingestor_relp
ln -fs ../vendor/logsearch-boshrelease/jobs/ingestor_syslog
ln -fs ../vendor/logsearch-boshrelease/jobs/kibana
ln -fs ../vendor/logsearch-boshrelease/jobs/log_parser
ln -fs ../vendor/logsearch-boshrelease/jobs/queue


ln -fs ../vendor/logsearch-boshrelease/packages/elasticsearch
ln -fs ../vendor/logsearch-boshrelease/packages/java7
ln -fs ../vendor/logsearch-boshrelease/packages/kibana
ln -fs ../vendor/logsearch-boshrelease/packages/logstash
ln -fs ../vendor/logsearch-boshrelease/packages/nginx
ln -fs ../vendor/logsearch-boshrelease/packages/redis

